#: localesetup:22
#, sh-format
msgid "USAGE: localesetup [locale]"
msgstr "UTILIZZO: impostazioni localizzazione [locale]"

#: localesetup:23
#, sh-format
msgid "EXAMPLE: localesetup en_US"
msgstr "ESEMPIO: localesetup en_US"

#: localesetup:28
#, sh-format
msgid "ERROR: Too many arguments"
msgstr "ERRORE: Troppi argomenti"

#: localesetup:34
#, sh-format
msgid "You need root privileges to run this"
msgstr "Devi avere i privilegi di amministratore per avviare questa applicazione"

#: localesetup:42
#, sh-format
msgid "Setting locale to $locale"
msgstr "Impostazione della localizzazione in $locale"

#: localesetup:44
#, sh-format
msgid "ERROR: $newlocale is not a valid locale"
msgstr "ERRORE: $newlocale non è una localizzazione valida"

#: localesetup:69
#, sh-format
msgid "System language configuration"
msgstr "Configurazione della lingua di sistema"

#: localesetup:71
#, sh-format
msgid "OK"
msgstr "OK"

#: localesetup:72
#, sh-format
msgid "Exit"
msgstr "Esci"

#: localesetup:74
#, sh-format
msgid "The current locale is $currentlocale."
msgstr "La Lingua corrente è $currentlocale."

#: localesetup:75
#, sh-format
msgid "Please select your preferred system language:"
msgstr "Per favore seleziona la lingua di sistema preferita:"

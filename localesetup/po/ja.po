#, sh-format
msgid "System language configuration"
msgstr "システム言語設定"

#: localesetup:71
#, sh-format
msgid "OK"
msgstr "OK"

#: localesetup:72
#, sh-format
msgid "Exit"
msgstr "終了"

#: localesetup:74
#, sh-format
msgid "The current locale is $currentlocale."
msgstr "現在ロケールは $currentlocale です"


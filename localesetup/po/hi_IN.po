#: localesetup:22
#, sh-format
msgid "USAGE: localesetup [locale]"
msgstr "उपयोग : localesetup [स्थानिकी]"

#: localesetup:23
#, sh-format
msgid "EXAMPLE: localesetup en_US"
msgstr "उदाहरण : localesetup hi_IN"

#: localesetup:28
#, sh-format
msgid "ERROR: Too many arguments"
msgstr "त्रुटि : अनावश्यक मानदंड"

#: localesetup:34
#, sh-format
msgid "You need root privileges to run this"
msgstr "इस हेतु रुट उपयोक्ता विशेषाधिकार होना आवश्यक है"

#: localesetup:42
#, sh-format
msgid "Setting locale to $locale"
msgstr "स्थानिकी $locale सेट की गई"

#: localesetup:44
#, sh-format
msgid "ERROR: $newlocale is not a valid locale"
msgstr "त्रुटि : $newlocale अमान्य स्थानिकी"

#: localesetup:69
#, sh-format
msgid "System language configuration"
msgstr "सिस्टम भाषा पुष्टिकरण"

#: localesetup:71
#, sh-format
msgid "OK"
msgstr "ठीक है"

#: localesetup:72
#, sh-format
msgid "Exit"
msgstr "बंद करें"

#: localesetup:74
#, sh-format
msgid "The current locale is $currentlocale."
msgstr "वर्तमान स्थानिकी $currentlocale है।"

#: localesetup:75
#, sh-format
msgid "Please select your preferred system language:"
msgstr "कृपया इच्छित सिस्टम भाषा चुनें :"

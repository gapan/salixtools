#: localesetup:22
#, sh-format
msgid "USAGE: localesetup [locale]"
msgstr "KULLANIM: localesetup [yerel ayar]"

#: localesetup:23
#, sh-format
msgid "EXAMPLE: localesetup en_US"
msgstr "ÖRNEK: localesetup en_US"

#: localesetup:28
#, sh-format
msgid "ERROR: Too many arguments"
msgstr "HATA: Çok fazla parametre var."

#: localesetup:34
#, sh-format
msgid "You need root privileges to run this"
msgstr "Bunu çalıştırmak için root yetkilerine sahip olmanız gerekmektedir"

#: localesetup:42
#, sh-format
msgid "Setting locale to $locale"
msgstr "Yerel ayarı $locale olarak ayarlanacak"

#: localesetup:44
#, sh-format
msgid "ERROR: $newlocale is not a valid locale"
msgstr "HATA: $newlocale geçerli bir yerel ayarı değil"

#: localesetup:69
#, sh-format
msgid "System language configuration"
msgstr "Sistem dili yapılandırması"

#: localesetup:71
#, sh-format
msgid "OK"
msgstr "TAMAM"

#: localesetup:72
#, sh-format
msgid "Exit"
msgstr "Çıkış"

#: localesetup:74
#, sh-format
msgid "The current locale is $currentlocale."
msgstr "Geçerli yerel ayarı $currentlocale."

#: localesetup:75
#, sh-format
msgid "Please select your preferred system language:"
msgstr "Lütfen tercih ettiğiniz sistem dilini seçiniz:"

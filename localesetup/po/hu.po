#: localesetup:22
#, sh-format
msgid "USAGE: localesetup [locale]"
msgstr "HASZNÁLAT: localesetup [lokalizáció]"

#: localesetup:23
#, sh-format
msgid "EXAMPLE: localesetup en_US"
msgstr "PÉLDA: localesetup en_US"

#: localesetup:28
#, sh-format
msgid "ERROR: Too many arguments"
msgstr "HIBA: Túl sok paraméter"

#: localesetup:34
#, sh-format
msgid "You need root privileges to run this"
msgstr ""

#: localesetup:42
#, sh-format
msgid "Setting locale to $locale"
msgstr "Lokalizáció beállítása erre: $locale"

#: localesetup:44
#, sh-format
msgid "ERROR: $newlocale is not a valid locale"
msgstr "HIBA: $newlocale nem érvényes lokalizáció"

#: localesetup:69
#, sh-format
msgid "System language configuration"
msgstr "Rendszernyelv beállítása"

#: localesetup:71
#, sh-format
msgid "OK"
msgstr "OK"

#: localesetup:72
#, sh-format
msgid "Exit"
msgstr "Kilépés"

#: localesetup:74
#, sh-format
msgid "The current locale is $currentlocale."
msgstr "A jelenlegi lokalizáció: $currentlocale."

#: localesetup:75
#, sh-format
msgid "Please select your preferred system language:"
msgstr "Válassza ki az előnyben részesített rendszernyelvet:"

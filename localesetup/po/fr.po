#: localesetup:22
#, sh-format
msgid "USAGE: localesetup [locale]"
msgstr "UTILISATION : localesetup [locale]"

#: localesetup:23
#, sh-format
msgid "EXAMPLE: localesetup en_US"
msgstr "EXEMPLE : localesetup fr_FR"

#: localesetup:28
#, sh-format
msgid "ERROR: Too many arguments"
msgstr "ERREUR : Trop d'arguments"

#: localesetup:34
#, sh-format
msgid "You need root privileges to run this"
msgstr "Vous avez besoin des droits de 'root' pour exécuter cette commande"

#: localesetup:42
#, sh-format
msgid "Setting locale to $locale"
msgstr "Affecte la locale à $locale"

#: localesetup:44
#, sh-format
msgid "ERROR: $newlocale is not a valid locale"
msgstr "ERREUR : $newlocale n'est pas une locale valide"

#: localesetup:69
#, sh-format
msgid "System language configuration"
msgstr "Configuration de la langue du système"

#: localesetup:71
#, sh-format
msgid "OK"
msgstr "Valider"

#: localesetup:72
#, sh-format
msgid "Exit"
msgstr "Quitter"

#: localesetup:74
#, sh-format
msgid "The current locale is $currentlocale."
msgstr "La locale actuelle du système est $currentlocale."

#: localesetup:75
#, sh-format
msgid "Please select your preferred system language:"
msgstr "Veuillez sélectionner votre nouvelle langue :"
